function onLine() {
  const online = document.getElementById('<%=@bundle.id%>');
  const onlineButton = online.querySelector('.btn-outline-success');
  const offlineButton = online.querySelector('.btn-outline-warning');
  const status = online.querySelector('.notification-actions');
  onlineButton.classList.remove('d-none');
  offlineButton.classList.add('d-none');
  status.innerHTML = "Set offline";
  //online.outerHTML('<%= j render "users/card-dashboard", space: @space %>');
}

onLine();
